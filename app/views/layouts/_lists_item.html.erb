<div class="row clearfix">
<p><%= lists_item.content %></p>
<%= link_to lists_lists_item_path(@lists, lists_item.id), method: :delete, data: { confirm: "Tem certeza?" } %>

<%= link_to "Completo", complete_lists_lists_item_path(@lists, lists_item.id), method: :patch %>

</div>
  <div class="lists_items">
   <p style="opacity: 0.4;"><strike><%= lists_items.content %></strike></p>
