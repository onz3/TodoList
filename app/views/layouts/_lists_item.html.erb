   <%= link_to "Delete", lists_lists_items_path(@lists, lists_items.id), method: :delete, data: { confirm: "Are you sure?" } %>
  </div>
 <% else %>
  <div class="complete">
   <%= link_to "Mark as Complete", complete_lists_lists_items_path(@lists, lists_items.id), method: :patch %>
  </div>
  <div class="lists_items">
   <p><%= lists_items.content %></p>
  </div>
  <div class="trash">
   <%= link_to "Delete", lists_lists_items_path(@lists, lists_items.id), method: :delete, data: { confirm: "Are you sure?" } %>
  </div>
 <% end %>